{
  "name": "cantina",
  "version": "2.0.7",
  "description": "**Cantina** is a modern web application that makes it easy to view, select, and manage weekly menus from your canteen. Whether you want to plan your meals in advance or contribute new dishes, Cantina provides a simple, intuitive interface for everyone â€” no login required.",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JGEsteves89/cantina.git"
  },
  "keywords": [
    "food",
    "cantina",
    "schedule",
    "plan"
  ],
  "author": "JGEsteves",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/JGEsteves89/cantina/issues"
  },
  "homepage": "https://github.com/JGEsteves89/cantina#readme",
  "scripts": {
    "dev:server": "tsx server.ts",
    "dev:client": "npm run build:client && vite --config vite.config.ts dev",
    "build": "rimraf dist && tsc -p tsconfig.json && npm run build:client && npm run build:server && npm run copy-files",
    "build:client": "vite build --outDir dist/client --ssrManifest",
    "build:server": "vite build --ssr src/client/entry-server.tsx --outDir dist/server",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "coverage": "vitest --coverage",
    "typecheck": "tsc --noEmit",
    "serve": "npm run build && cross-env NODE_ENV=production node ./dist/server",
    "serve:local": "vite serve",
    "clean": "rimraf dist/",
    "copy-files": "copyfiles \"public/**/*\" dist && copyfiles -u 2 \"dist/client/**/*\" dist && copyfiles -u 2 \"dist/client/assets/**/*\" dist/public",
    "format": "prettier --write src types",
    "docker:build": "node scripts.cjs dockerBuild",
    "docker:push": "node scripts.cjs dockerPush",
    "docker:release": "node scripts.cjs dockerRelease",
    "docker:pull": "node scripts.cjs dockerPull",
    "docker:stop": "node scripts.cjs dockerStop",
    "docker:compose": "node scripts.cjs dockerCompose",
    "docker:restart": "node scripts.cjs dockerRestart",
    "release:patch": "node scripts.cjs releasePatch",
    "release:minor": "node scripts.cjs releaseMinor",
    "release:major": "node scripts.cjs releaseMajor"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/material": "^7.3.5",
    "@tanstack/react-query": "^5.90.6",
    "@types/serve-static": "^1.15.4",
    "autoprefixer": "^10.4.16",
    "compression": "^1.8.1",
    "console-stamp": "^3.1.2",
    "cross-env": "^7.0.3",
    "dotenv": "^17.2.3",
    "express": "^4.21.2",
    "lucide-react": "^0.552.0",
    "morgan": "^1.10.1",
    "my-json-server-lib": "^1.0.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.18.0",
    "serve-static": "^1.15.0",
    "tailwind-merge": "^3.3.1",
    "tailwindcss-animate": "^1.0.7",
    "tsx": "^4.20.6",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^14.0.0",
    "@types/compression": "1.7.4",
    "@types/concurrently": "6.4.0",
    "@types/eslint": "8.44.6",
    "@types/express": "^4.17.20",
    "@types/morgan": "^1.9.10",
    "@types/node": "^24.10.0",
    "@types/react": "^18.2.34",
    "@types/react-dom": "^18.2.14",
    "@typescript-eslint/eslint-plugin": "^6.9.1",
    "@typescript-eslint/parser": "^6.9.1",
    "@vitejs/plugin-react": "^4.1.1",
    "@vitest/coverage-v8": "^4.0.7",
    "concurrently": "8.2.2",
    "copyfiles": "^2.4.1",
    "eslint": "^8.52.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jsdom": "^22.1.0",
    "postcss": "8.4.31",
    "prettier": "^3.0.3",
    "rimraf": "^5.0.5",
    "tailwindcss": "3.3.5",
    "typescript": "5.2.2",
    "vite": "^7.2.0",
    "vitest": "^4.0.7"
  }
}
